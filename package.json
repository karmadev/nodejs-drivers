{
  "name": "@karmalicious/nodejs-drivers",
  "version": "7.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "repository": "git@github.com:karmadev/nodejs-drivers.git",
  "author": "Marcus Nielsen <marcus@karma.life>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@google-cloud/pubsub": "^0.19.0",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/jsreport-core": "^1.5.0",
    "@types/sequelize": "^4.27.24",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.9",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jsreport": "^2.1.1",
    "jsreport-core": "^2.1.0",
    "jsreport-jsrender": "^2.0.0",
    "jsreport-phantom-pdf": "^2.1.3",
    "pg": "^7.4.3",
    "rxjs": "^6.2.2",
    "sequelize": "^4.38.0",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "nodemon build/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "NODE_ENV=test jest",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "precommit": "lint-staged",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
