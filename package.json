{
  "name": "@karmalicious/nodejs-drivers",
  "version": "2.0.1",
  "main": "build/index.js",
  "repository": "git@github.com:karmadev/nodejs-drivers.git",
  "author": "Marcus Nielsen <marcus@karma.life>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "amqplib": "^0.5.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsreport": "^1.9.2",
    "jsreport-core": "^1.5.1",
    "jsreport-jsrender": "^1.0.2",
    "jsreport-phantom-pdf": "^1.4.6",
    "pg": "^7.4.3",
    "sanctuary": "^0.15.0",
    "sequelize": "^4.37.10",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.6",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.0.0",
    "@types/jsreport-core": "^1.5.0",
    "@types/sequelize": "^4.27.20",
    "@types/winston": "^2.3.9",
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.1.3",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.1"
  },
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "nodemon build/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "NODE_ENV=test jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "precommit": "lint-staged",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
